{
  "expo": {
    "name": "Beacon",
    "slug": "beacon",
    "version": "0.0.1",
    "scheme": "beacon",
    "orientation": "portrait",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "sdkVersion": "54.0.0",
    "platforms": ["ios", "android", "web"],
    "experiments": {
      "newArchEnabled": false
    },
    "android": {
      "package": "com.hd_johnny.beaconui"
    },
    "ios": {
      "bundleIdentifier": "com.hd-johnny.beaconui"
    },
    "web": {
      "bundler": "metro",
      "output": "dist"
    }
  }
}
